class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        
        vector<int>av;

        for(int i=0;i<nums.size()-1;i++){
            for(int j=0;j<nums.size();j++){
                if(nums[i]+nums[j]==target){
                    av.push_back(nums[i]);
                    av.push_back(nums[j]);
                }
            }
        }
        return av;
    }
};
